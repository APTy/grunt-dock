#
# Nginx default config file
#

upstream node {
  server 172.17.0.156:8080;
}

server {
  error_log /dev/stderr;
  access_log /dev/stdout;
  server_name  localhost;
  listen 80;
  root /bundle;
  index index.html;
  location / {
    try_files $uri $uri/ index.html;
  }
  location /node {
    proxy_pass http://node;
  }
}

